<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#101217"
    android:visibility="visible"
    tools:context=".menumaestro"
    tools:visibility="visible">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="413dp"
        android:layout_height="849dp"
        android:background="#00322626"
        android:tint="#B50C0C0C"
        app:srcCompat="@drawable/fofo"
        tools:ignore="ContentDescription,MissingConstraints,UseAppTint,VisualLintBounds,ImageContrastCheck"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#090A0C"
        app:srcCompat="@drawable/kbeson"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-100dp"
        tools:ignore="ContentDescription,MissingConstraints,UseAppTint,VisualLintBounds" />

    <TextView
        android:id="@+id/textView"
        style="@style/Widget.AppCompat.TextView"
        android:layout_width="278dp"
        android:layout_height="64dp"
        android:layout_marginTop="56dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="CLASES"
        android:textColor="#D8D4D4"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="98dp"
        android:layout_height="158dp"
        app:srcCompat="@drawable/fototio"
        tools:ignore="ContentDescription,MissingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-30dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginTop="32dp"
        android:text="Maestro"
        android:textColor="#FBF1F1"
        android:textSize="24sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="98dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="211dp"
        android:layout_height="61dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/button_styles"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Crear Nueva Clase +"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck,TextSizeCheck"
        tools:layout_editor_absoluteX="111dp" />

</androidx.constraintlayout.widget.ConstraintLayout>